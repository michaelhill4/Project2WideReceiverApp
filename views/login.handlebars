<div class="jumbotron" id="jumbtron">
  <div>
    <h2>Fast Food Randomizer</h2>
  </div>
  <div>
    <h3>Don't know what you want to eat? Login/signup below to let us help you choose.</h3>
  </div>
</div>

<div class="justify-space-between-md">
  <div class="login-card">
    <h2 class="page-title">Login</h2>
    <form class="form login-form">
      <div class="form-group">
        <label for="email-login">Email:</label>
        <input class="form-input" type="text" id="email-login" />
      </div>
      <div class="form-group">
        <label for="password-login">Password:</label>
        <input class="form-input" type="password" id="password-login" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">login</button>
      </div>
    </form>
  </div>
  <div class="login-card">
    <h2 class="page-title">Signup</h2>
    <form class="form signup-form">
      <div class="form-group">
        <label for="username-signup">Username:</label>
        <input class="form-input" type="text" id="username-signup" />
      </div>
      <div class="form-group">
        <label for="email-signup">Email:</label>
        <input class="form-input" type="text" id="email-signup" />
      </div>
      <div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">signup</button>
      </div>
    </form>
  </div>
  <img src="/images/hungry-guy.jpg" alt="hungry person">
    <img src="/images/tacobell.jpg" alt="tacos">
    <img src="/images/chikfila sandwich.jpg" alt="chicken sandwiches">
    <img src="/images/big mac.jpg" alt="burgers">
    <img src="/images/subway.jpg" alt="pictures of subs">
</div>

<script src="./js/login.js"></script>